{
  "output": {
    "filePath": "linkarooie-packed.txt",
    "style": "plain",
    "showLineNumbers": true,
    "fileSummary": true,
    "directoryStructure": true,
    "removeComments": false,
    "removeEmptyLines": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.css",
    "*.json",
    "*.js",
    "*.ts",
    "public/**/*.html",
    "public/**/*.svg",
    "README.md",
    "requirements.md",
    "index.html",
    ".env.example"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "linkarooie-packed.txt",
      "src/data/profiles/*",
      ".env"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "cl100k_base"
  }
}